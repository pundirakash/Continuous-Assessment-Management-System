{
    "version": 2,
    "name": "cams-backend",
    "functions": {
      "api/**/*.js": {
        "maxDuration": 60
      },
      "app.js": {
        "maxDuration": 60
      }
    },
    "builds": [
       { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/app.js" }
    ]
 }